{
  "name": "cleaning-landing",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:pages": "node scripts/run-pages.mjs dev",
    "build": "astro build",
    "build:pages": "node scripts/run-pages.mjs build",
    "preview": "astro preview",
    "preview:pages": "node scripts/run-pages.mjs preview",
    "simulate:pages": "node scripts/simulate-pages.mjs",
    "gallery:fetch": "node scripts/fetch-gallery.mjs",
    "check:copy": "node scripts/check-mojibake.mjs",
    "test": "pnpm test:unit",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:a11y": "playwright test a11y.spec.ts",
    "typecheck": "astro check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "check": "pnpm format && pnpm lint && pnpm typecheck",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/react": "^4.4.2",
    "@astrojs/sitemap": "3.6.0",
    "@astrojs/tailwind": "6.0.2",
    "@fontsource/inter": "^5.2.8",
    "@fontsource/playfair-display": "^5.2.8",
    "@radix-ui/react-slot": "^1.2.4",
    "astro": "5.16.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "lenis": "^1.3.17",
    "lucide-react": "^0.562.0",
    "mini-svg-data-uri": "^1.4.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "9.39.2",
    "@playwright/test": "^1.57.0",
    "@types/node": "25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/ui": "^4.0.16",
    "astro-eslint-parser": "^1.2.2",
    "autoprefixer": "10.4.23",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro,json,md,mdx,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,astro}": [
      "eslint --fix"
    ]
  },
  "volta": {
    "node": "22.21.1"
  }
}
